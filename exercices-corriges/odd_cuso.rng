   <define name="tei_att.global.attribute.xmllang">
      <optional>
         <attribute name="xml:lang">
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(langue) indique la langue du contenu de l'élément en utilisant les codes du <a xmlns="http://www.w3.org/1999/xhtml"
                  href="https://www.ietf.org/rfc/rfc3066.txt">RFC 3066</a>
            </a:documentation>
            <choice>
               <data type="language"/>
